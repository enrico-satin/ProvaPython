def exercicio3():
  entrada = input("Insira 'a' para Globo, 'b' para SBT e 'Z' ou 'z' para o finalizar: ")
  while entrada != 'z':
    if entrada == 'Z':
      break
    elif entrada == 'a':
      print("Globo")
    elif entrada == 'b':
      print("SBT")
    else:
      print("Invalid")
    entrada = input()
